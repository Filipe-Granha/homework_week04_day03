<h1>All the students at Hogwarts</h1>
<div>
<img style="border: #000000 6px outset; width: 400px; display: block; margin-left: auto; margin-right: auto;" src="https://lh3.googleusercontent.com/-7fKCy2l1Oa0/Up6jIeDp1-I/AAAAAAAABDY/yZuxct7TiRs/w720-h352-no/GiveawayLineup_Small.jpg">
</div>
<% @students.each do |student| %>
  <p>Name: <%= student.first_name() %> <%= student.second_name() %></p>
  <p>House: <%= student.house().name %></p>
  <p>Age: <%= student.age() %></p>
  <hr>
<% end %>

